let jenny = "Kim Jenny"
let Lisa = "La Lisa"
let Jisoo = "Kim Jisoo"
let Rose = "Park Chae-Yong"
let BlackPink = [jenny, Lisa, Jisoo, Rose]
BlackPink[0]   // Kim Jenny
print(BlackPink[1])   // La Lisa

var evenNumbers = [2, 4, 6, 8]
// songsì— ë”°ë¡œ ìœ í˜•ì„ ì§€ì •í•´ì£¼ê¸° ì•Šì•˜ê¸°ì— ë‹¨ì§€ ì˜ˆì¸¡ë§Œìœ¼ë¡œ stringìœ í˜•ì´ë¼ê³  í‘œì‹œ
var songs = ["ë§ˆì§€ë§‰ì²˜ëŸ¼", "ëšœë‘ëšœë‘", "Pink Venom"]
